        "array = [\"水\",\"金\",\"地\",\"火\",\"木\",\"土\",\"天\",\"海\",\"冥\"]\n",
        "\n",
        "count = 0 #何回繰り返したかをカウント\n",
        "\n",
        "while count < 2: #2回繰り返す\n",
        "  for value in array:\n",
        "    print(value)\n",
        "\n",
        "  count += 1"
